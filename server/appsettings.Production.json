{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "Token": "${JWT_SECRET}",
    "Issuer": "verbatim-auth",
    "Audience": "verbatim-api"
  },
  "MongoDbSettings": {
    "ConnectionString": "${MONGODB_CONNECTION_STRING}",
    "DatabaseName": "verbatim"
  },
  "FuzzySearchMicroservice": {
    "ConnectionString": "${FUZZY_SEARCH_URL}"
  }
}